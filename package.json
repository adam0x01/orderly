{
  "name": "orderly",
  "displayName": "orderly",
  "description": "Generate numerical or alphabetical order list for your text.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.71.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:orderly.sequenceStyle1",
    "onCommand:orderly.sequenceStyle2"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "orderly.sequenceStyle1",
        "title": "Orderly: add sequence 1. 2. 3."
      },
      {
        "command": "orderly.sequenceStyle2",
        "title": "Orderly: add sequence 1) 2) 3)"
      },
      {
        "command": "orderly.reorder",
        "title": "Orderly: reorder"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "pnpm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "pnpm run compile && pnpm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.71.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4",
    "@vscode/test-electron": "^2.1.5"
  }
}
